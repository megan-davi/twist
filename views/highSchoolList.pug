extends layout

block content
  .row
    .col-lg-6
      h1= title
      h5 Click to view more information about a high school, edit or delete it.
    .col-lg-6.right
      a.btn.btn-lg.btn-success(href='/twist/highSchool/create', role='button') Create new High School

  br
  .row
    .col-lg-12
      ul
        each highSchool in highSchoolList
          li.horz
            a(href=highSchool.url) #{highSchool.highSchoolName}

        else
          li There are no high schools.
