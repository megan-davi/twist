extends layout

block content
  .row
    .col-lg-6
      h1= title
    .col-lg-6.right
      a.btn.btn-lg.btn-success(href='/twist/session/create', role='button') Create new Session

  br
  .row
    .col-lg-12
      ul
        each session in sessionList
          li.horz
            #{session.time}

        else
          li There are no sessions.
